{
  "presets": [
    ["env", {
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7"]
        }
    }],
    "stage-0",
    "react"
  ],
    "plugins": [
        "add-module-exports",
        [
            "import",
            [
                {
                    "libraryName": "antd",
                    "style": true
                }
            ]
        ]
    ],
    "env": {
        "production": {
        },
        "development": {
            "plugins": [
                "react-hot-loader/babel"
            ]
        }
    }
}
